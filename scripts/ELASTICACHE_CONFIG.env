# ============================================
# AWS ElastiCache (Redis) Configuration
# ============================================
# Copy these values to your .env file
# ============================================

# Primary endpoint from AWS ElastiCache console
# Your endpoint: master.redis-cache-server.wwhabv.aps1.cache.amazonaws.com:6379
ELASTICACHE_ENDPOINT=master.redis-cache-server.wwhabv.aps1.cache.amazonaws.com:6379

# Port (usually 6379 for Redis)
# Note: If port is included in ELASTICACHE_ENDPOINT, this will be ignored
ELASTICACHE_PORT=6379

# Auth Token (AUTH token) from ElastiCache setup
# Your auth token: AdeptAiPro_2025_Redis
ELASTICACHE_AUTH_TOKEN=AdeptAiPro_2025_Redis

# SSL/TLS Configuration
# Set to 'true' if you enabled Encryption in-transit on your ElastiCache cluster
# Most ElastiCache clusters use non-SSL by default
REDIS_SSL=false

# Disable Redis (set to 'true' to use in-memory cache only)
# Set to 'false' to use ElastiCache
DISABLE_REDIS=false

# ============================================
# Search System Configuration
# ============================================
# Force loading all candidates from DynamoDB on startup
FORCE_FULL_LOAD=true

# Number of parallel workers for loading/caching/matching
# Adjust based on your server's CPU cores and memory
# Recommended: 8-16 for production
MAX_WORKERS=16

# Threshold for parallel matching (number of candidates)
# Use parallel matching for datasets larger than this threshold
PARALLEL_MATCHING_THRESHOLD=10000

